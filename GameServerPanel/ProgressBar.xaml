<Window x:Class="GameServerPanel.ProgressBar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameServerPanel"
        mc:Ignorable="d"
        Title="进度条" Height="137" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <ProgressBar Height="24" HorizontalAlignment="Center" x:Name="progressBar" VerticalAlignment="Center" Foreground="#FF2EAFF1" Grid.Row="1" Width="200" />
        <Label x:Name="operationId" Content="" HorizontalAlignment="Left" Margin="12,0,0,0" VerticalAlignment="Center"/>
        <Label x:Name="FileName" Content="" HorizontalAlignment="Left" Margin="12,0,0,0" VerticalAlignment="Center" Grid.Row="2"/>
    </Grid>
</Window>
